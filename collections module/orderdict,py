from collections import OrderedDict

print("This is dict:/n")

print("\nThis is Ordereddict")
od = OrderedDict()
od['a']=1
od['b']=2
od['c']=3
od['d']=4

print("before deleting")
for key,values in od.items():
    print(key,values)


od.pop('a')


od['a']=1

print("after re-inserting")
for key,values in od.items():
    print(key,values)